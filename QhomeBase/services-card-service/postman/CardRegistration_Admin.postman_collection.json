              "host": [
                "{{cardServiceBaseUrl}}"
              ],
              "path": [
                "api",
                "register-service",
                "admin",
                "vehicle-registrations",
                "{{vehicleRegistrationId}}",
                "reject"
              ]
            }
          }
        },
        {
          "name": "VNPay Return Callback",
          "request": {
            "method": "GET",
